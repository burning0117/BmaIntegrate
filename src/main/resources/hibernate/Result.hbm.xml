<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.result" table="result" catalog="bma">
        <id name="rid" type="java.lang.Long" length="5">
            <column name="rid"></column>
            <generator class="identity"></generator>
        </id>
        <property name="oid" type="java.lang.Long"></property>
        <property name="pid" type="java.lang.Long"></property>
        <property name="state" type="java.lang.Integer"></property>
    </class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.AdminUser" table="adminUser" catalog="bma">
        <id name="auid" type="java.lang.Long" length="5">
            <column name="auid"></column>
            <generator class="identity"></generator>
        </id>
        <property name="username" type="java.lang.String" length="20"></property>
        <property name="password" type="java.lang.String" length="20"></property>
    </class>
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
        "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
    <package name="adminCategory" namespace="/" extends="struts-default">
        <action name="adminCategoryAction_*" method="{1}" class="action.AdminUserAction">
            <result name="findAll">/admin/category/list.jsp</result>
            <result name="saveSuccess" type="redirectAction">adminCategoryAction_findAll</result>
            <result name="deleteSuccess" type="redirectAction">adminCategoryAction_findAll</result>
            <result name="editSuccess" type="redirectAction">adminCategoryAction_findAll</result>
            <result name="updateSuccess" type="redirectAction">adminCategoryAction_findAll</result>
        </action>
    </package>
</struts>
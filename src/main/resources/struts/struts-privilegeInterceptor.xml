<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
        "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
    <package name="privilegeInterceptor" namespace="/" extends="struts-default">
        <interceptors>
            <interceptor name="privilegeInterceptor" class="filter.PrivilegeInterceptor">
            </interceptor>
        </interceptors>
        <global-results>
            <result name="msg">/WEB-INF/jsp/msg/msg.jsp</result>
            <result name="login">/admin/index.jsp</result>
        </global-results>
    </package>
</struts>
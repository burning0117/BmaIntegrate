<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
        "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
    <package name="cart" namespace="/" extends="struts-default">
        <action name="cartAction_*" method="{1}" class="action.CartAction">
            <result name="addCart">WEB-INF/jsp/cart/cart.jsp</result>
            <result name="removeCart">WEB-INF/jsp/cart/cart.jsp</result>
            <result name="clearCart">WEB-INF/jsp/cart/cart.jsp</result>
            <result name="myCart">WEB-INF/jsp/cart/cart.jsp</result>
        </action>
    </package>
</struts>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
        "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
    <package name="adminProduction" namespace="/" extends="struts-default">
        <action name="adminProductionAction_*" method="{1}" class="action.AdminProductionAction">
            <result name="findAll">/admin/product/list.jsp</result>
            <result name="addPageSuccess">/admin/product/add.jsp</result>
            <result name="saveSuccess" type="redirectAction">adminProduction_findAll.action?page=1</result>
            <result name="deleteSuccess" type="redirectAction">adminProduction_findAll.action?page=1</result>
            <result name="editSuccess" type="redirectAction">adminProduction_findAll.action?page=1</result>
        </action>
    </package>
</struts>